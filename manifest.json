  "features": [
    "Piattaforma professionale FSGC",
    "Analisi match avanzata con AI",
    "Sistema di rating giocatori FIFA-compliant",
    "Export report professionali PDF/Excel",
    "Community engagement e sentiment analysis",
    "Dashboard federazione con KPI",
    "Rete globale squadre e scout",
    "Sincronizzazione cloud sicura",
    "Conformità GDPR e privacy",
    "Integrazione sistemi federazione"
  ],
  "protocol_handlers": [
    {
      "protocol": "web+soccerbox",
      "url": "./?import=%s"
    },
    {
      "protocol": "web+fsgc",
      "url": "./?fsgc=%s"
    }
  ],
  "share_target": {
    "action": "./",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url",
      "files": [
        {
          "name": "match_data",
          "accept": ["application/json", "text/csv", ".xlsx"]
        }
      ]
    }
  },
  "file_handlers": [
    {
      "action": "./",
      "accept": {
        "application/json": [".json"],
        "text/csv": [".csv"],
        "application/vnd.openxmlformats-officedocument.spreadsheetml.sheet": [".xlsx"],
        "application/pdf": [".pdf"],
        "text/markdown": [".md"]
      }
    }
  ],
  "edge_side_panel": {
    "preferred_width": 450
  },
  "launch_handler": {
    "client_mode": "navigate-existing"
  },
  "display_override": ["window-controls-overlay", "standalone"],
  "orientation": "any",
  "permissions": [
    "notifications",
    "persistent-storage",
    "background-sync"
  ]
}
